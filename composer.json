{
  "name": "bambolee-digital/event-user-manager",
  "description": "A user event management system for Laravel",
  "version": "1.0.0",
  "type": "library",
  "license": "MIT",
  "authors": [
      {
          "name": "Bambolee Digital",
          "email": "contact@bambolee.digital"
      }
  ],
  "require": {
      "php": "^8.1",
